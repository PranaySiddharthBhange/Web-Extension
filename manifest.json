{
    "manifest_version": 3,
    "name": "Dictionary",
    "version": "1.0",
    "description": "Display selected word meaning on a web page",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "book16.png",
        "24": "book24.png",
        "32": "book32.png",
        "64": "book64.png",
        "128": "book128.png",
        "512": "book512.png"

      }
    }
  }
  
